    <Resource Include="WPF\Images\guideBackground.png" />
    <Resource Include="WPF\Images\home.png" />
    <Resource Include="WPF\Images\mountain-city.png" />
    <Resource Include="WPF\Images\question-mark.png" />
    <Resource Include="WPF\Images\star.png" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <Page Update="WPF\Views\Guest2Views\Guest2MainView.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
    </Page>
    <Page Update="WPF\Views\Guest2Views\TourReservationView.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
    </Page>
    <Page Update="WPF\Views\Guest2Views\TourSearchView.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
    </Page>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="WPF\Validations\" />
  </ItemGroup>

</Project>
