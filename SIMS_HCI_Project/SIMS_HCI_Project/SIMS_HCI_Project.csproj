  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\Images\back.png" />
    <Resource Include="Resources\Images\help.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="DotNetProjects.WpfToolkit.DataVisualization" Version="6.1.94" />
    <PackageReference Include="Extended.Wpf.Toolkit" Version="4.5.0" />
    <PackageReference Include="LiveCharts.Wpf" Version="0.9.7" />
    <PackageReference Include="Microsoft.Xaml.Behaviors.Wpf" Version="1.1.39" />
    <PackageReference Include="Syncfusion.Pdf.Wpf" Version="21.2.5" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\Images\booking.png" />
    <Resource Include="Resources\Images\chat.png" />
    <Resource Include="Resources\Images\goldenHouse.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\Images\logout.png" />
    <Resource Include="Resources\Images\maintenance.png" />
    <Resource Include="Resources\Images\menu.png" />
    <Resource Include="Resources\Images\patternCircle.jpg" />
    <Resource Include="Resources\Images\patternLines.png" />
    <Resource Include="Resources\Images\patternTriangle.jpg" />
    <Resource Include="Resources\Images\previous.png" />
    <Resource Include="Resources\Images\profil.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\Images\rating.png" />
    <Resource Include="Resources\Images\reserve.png" />
    <Resource Include="Resources\Images\search.png" />
    <Resource Include="Resources\Images\TourSearchHelp.png" />
    <Resource Include="Resources\Images\user.png" />
    <Resource Include="Resources\Images\warning.png" />
