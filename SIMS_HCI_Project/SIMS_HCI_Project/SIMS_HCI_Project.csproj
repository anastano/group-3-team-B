    <None Remove="Resources\Images\help.png" />
    <None Remove="Resources\Images\logout.png" />
    <None Remove="Resources\Images\maintenance.png" />
    <None Remove="Resources\Images\menu.png" />
    <None Remove="Resources\Images\patternCircle.jpg" />
    <None Remove="Resources\Images\patternLines.png" />
    <None Remove="Resources\Images\patternTriangle.jpg" />
    <None Remove="Resources\Images\previous.png" />
    <None Remove="Resources\Images\profil.png" />
    <None Remove="Resources\Images\rating.png" />
    <None Remove="Resources\Images\reserve.png" />
    <None Remove="Resources\Images\search.png" />
    <None Remove="Resources\Images\TourSearchHelp.png" />
